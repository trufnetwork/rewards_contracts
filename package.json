  "scripts": {
    "test:unit": "npx hardhat test test/RewardDistributor.ts",
    "test:unit:factory": "npx hardhat test test/RewardDistributorWithFactory.ts",
    "test:gas": "npx hardhat test --grep 'Gas Fee without GnosisSafe'",
    "test:lib_reward": "npx hardhat test peripheral/lib/test/reward.test.ts",
    "test:lib_gonsis": "npx hardhat test peripheral/lib/test/sepolia_gnosis.test.ts",
    "deploy-factory:sepolia": "npx hardhat --network sepolia deploy-factory",
    "deploy-clone:sepolia": "npx hardhat --network sepolia deploy-clone",
    "verify:sepolia": "npx hardhat --network sepolia ignition verify chain-11155111",
    "fork:deploy_safe": "npx hardhat run scripts/deploy_safe_fork.ts --config hardhat.config.fork.sepolia.ts",
    "dev:node": "npx hardhat node",
    "dev:fork:sepolia": "npx hardhat node --config hardhat.config.fork.sepolia.ts",
    "dev:fork:sepolia:cli": "npx hardhat node --fork $(grep SEPOLIA_RPC .env | cut -d '=' -f2) --fork-block-number 7606599",
    "dev:fork:sepolia:anvil": "anvil --fork-url $(grep SEPOLIA_RPC .env | cut -d '=' -f2) --fork-chain-id 11155111 --fork-block-number 7606599 --mnemonic \"$(grep SEPOLIA_MNEMONIC .env | cut -d '=' -f2)\"",
    "build": "tsc build --noEmit",
    "poster": "npx tsx peripheral/poster/cli.ts"
  },
  "scriptsComments": {
    "test:unit": "reward contract unit tests",
    "test:gas": "display gas consumption of interacting with Reward contract(without GnosisSafe)",
    "redeploy:local": "redeploy Reward contract(with token) to local Hardhat node; need to run Hardhat node in a different terminal",
    "redeploy:sepolia": "redeploy Reward contract(with tokwn) to Sepolia test network",
    "fork:sepolia": "fork Sepolia network locally",
    "fork:deploy_safe": "deploy a Gnosis Safe wallet on forked local Sepolia network"
  },
